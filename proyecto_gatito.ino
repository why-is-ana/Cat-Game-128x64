const int gatitoIzquierda = 36;
const int gatitoDerecha = 39;


void setup()   {

  Serial.begin(9600); // definimos la velocidad de comunicacion.

  // declaramos ambos pines como entradas.
  pinMode(gatitoIzquierda, INPUT);
  pinMode(gatitoDerecha, INPUT);


  delay(250); // Delay dejado para esperar a que inicie el Display OLED
  display.begin(i2c_Address, true); // Address 0x3C default
  display.display(); // inicializamos el Display OLED

}


void loop() {

  /* --- Si no hemos presionado ningun boton --- */
  if (digitalRead(gatitoIzquierda) == LOW && digitalRead(gatitoDerecha) == LOW) {
    display.clearDisplay(); // borramos la pantalla
    display.drawBitmap(0, 0,  gatoEnReposo, 128, 64, 1); // imprimimos al gato en reposo
